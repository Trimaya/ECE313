#1s PGM
loop steps = 1
assign name=BOthick n.value=(0.010)
assign name=TLthick n.value=(0.008)
assign name=TOthick n.value=(0.003)

LOOP steps=6
assign name=Vn n.value=(5, 6, 7, 8, 9 , 10)
go victorydevice simflags = "-P 10"

mesh inf=BO$'BOthick'TO$'TOthick'TL$'TLthick'_SONOS.str

load infile=BO$'BOthick'TO$'TOthick'TL$'TLthick'_$'Vn'_SONOS_PGM_10ns.str master
contact name=gate n.poly

models srh fermi FNORD PF.NITRIDE NEARFLG print

method carriers=2

solve init
solve prev

solve name=drain vstep=0.5 vfinal=1.0

log outf=BO$'BOthick'TO$'TOthick'TL$'TLthick'_$'Vn'_SONOS_PGM_10ns.log
solve name=gate vgate=-4 vstep=0.2 vfinal=10

extract name="New_vt_1s" (xintercept(maxslope(curve(abs(v."gate"),abs(i."drain")))) - abs(ave(v."drain"))/2.0)
log off

go victorydevice simflags = "-P 10"

mesh inf=BO$'BOthick'TO$'TOthick'TL$'TLthick'_SONOS.str


load infile=BO$'BOthick'TO$'TOthick'TL$'TLthick'_$'Vn'_SONOS_PGM_100ns.str master
